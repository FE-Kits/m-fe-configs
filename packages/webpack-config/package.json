{
  "name": "@m-fe/webpack-config",
  "version": "0.1.12",
  "description": "Shareable Webpack Configuration for all micro-frontend projects.",
  "repository": "https://github.com/wx-chevalier/m-fe-configs",
  "author": "wx-chevalier",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf example/build && cross-env NODE_ENV=production webpack --config ./example/scripts/webpack/webpack.config.prod.js -p",
    "build:node": "rm -rf ./example-node/build && cross-env NODE_ENV=production webpack --config ./example-node/webpack.config.js -p --progress",
    "dev": "webpack-dev-server --config ./example/scripts/webpack/webpack.config.dev.js --hot --progress",
    "dev:node": "webpack --config ./example-node/webpack.config.js --watch --progress"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.12.0",
    "@svgr/webpack": "^5.2.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "3.4.2",
    "file-loader": "5.1.0",
    "fork-ts-checker-webpack-plugin": "^4.0.5",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "4.0.0-beta.2",
    "html-webpack-template": "^6.2.0",
    "lazy-compile-webpack-plugin": "^0.1.11",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "less-plugin-rewrite-import": "^0.1.1",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "3.0.0",
    "react-dom": "^16.13.0",
    "react-hot-loader": "^4.12.19",
    "resolve-url-loader": "3.1.1",
    "style-loader": "1.1.3",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^2.3.5",
    "thread-loader": "^2.1.3",
    "ts-loader": "6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "^3.0.0",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "webpack-theme-color-replacer": "^1.3.5",
    "workbox-webpack-plugin": "^5.0.0",
    "workerize-loader": "^1.1.0"
  },
  "devDependencies": {
    "@m-fe/babel-preset": "^0.0.3",
    "@types/webpack-merge": "^4.1.5",
    "antd": "^4.0.2",
    "antd-mobile": "^2.3.1",
    "cross-env": "^7.0.2",
    "prepack-webpack-plugin": "^1.1.2",
    "react": "^16.13.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
